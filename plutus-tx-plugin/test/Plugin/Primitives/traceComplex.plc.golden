        (vardecl True Bool) (vardecl False Bool)
      )
    )
    (termbind
      (strict)
      (vardecl trace (all a (type) (fun (con string) (fun a a))))
      (builtin trace)
    )
    (termbind
      (nonstrict)
      (vardecl trace (all a (type) (fun (con string) (fun a a))))
